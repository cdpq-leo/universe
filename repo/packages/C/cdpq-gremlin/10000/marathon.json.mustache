{
  "id": "/{{service.name}}",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [
      {
        "containerPort": 8182,
        "hostPort": 0,
        "labels": {
          "VIP_0": "/{{service.name}}:8182"
        },
        "protocol": "tcp",
        "servicePort": 10114
      }
    ],
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "{{resource.assets.container.docker.image}}",
      "forcePullImage": true,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": {{service.cpus}},
  "disk": {{service.disk}},
  "healthChecks": [],
  "instances": 1,
  "maxLaunchDelaySeconds": 3600,
  "mem": {{service.mem}},
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "fetch": [],
  "env": {
      {{#placement.region}}
      "SERVICE_REGION": "{{placement.region}}"
      {{/placement.region}}
  }
}
