{
  "id": "/{{service.name}}",
  "backoffFactor": {{service.back_off_factor}},
  "backoffSeconds": {{service.back_off_seconds}},
  "mem": {{service.mem}},
  "cpus": {{service.cpus}},
  "disk": {{service.disk}},
  "instances": {{service.instances}},
  "maxLaunchDelaySeconds": {{service.max_launch_delay_seconds}},
  "container": {
    "type": "DOCKER",
    "docker": {
      "forcePullImage": true,
      "image": "{{resource.assets.container.docker.image}}",
      "parameters": [],
      "privileged": false
    },
    "volumes": [
      {
        "containerPath": "{{volumes.data_host_volume.container_path}}",
        "hostPath": "{{volumes.data_host_volume.host_path}}",
        "mode": "{{volumes.data_host_volume.mode}}"
      }
    ],
    "portMappings": [
      {
        "containerPort": {{networking.container_port}},
        "hostPort": {{networking.host_port}},
        "name": "{{networking.name}}",
        "protocol": "tcp",
        "servicePort": {{networking.service_port}}
      }
    ]
  },
  "env": {
    "VIEWER_URL": "{{environment.VIEWER_URL}}",
    "MARKLOGIC_USERNAME": "{{environment.MARKLOGIC_USERNAME}}",
    "MARKLOGIC_PASSWORD": "{{environment.MARKLOGIC_PASSWORD}}",
    "MARKLOGIC_URL": "{{environment.MARKLOGIC_URL}}",
    "KAFKA_BOOTSTRAP_SERVERS": "{{environment.KAFKA_BOOTSTRAP_SERVERS}}",
    "MARKLOGIC_DB": "{{environment.MARKLOGIC_DB}}",
    "KAFKA_CONSUMER_TOPIC": "{{environment.KAFKA_CONSUMER_TOPIC}}"
  },
  "executor": "",
  "labels": {},
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "version": "2019-05-23T11:07:26.750Z",
  "versionInfo": {
    "lastScalingAt": "2019-05-23T11:07:26.750Z",
    "lastConfigChangeAt": "2019-05-23T11:07:26.750Z"
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "fetch": [
    {
      "uri": "{{service.artifact_URI.uri}}",
      "extract": true,
      "executable": false,
      "cache": false
    }
  ]
}