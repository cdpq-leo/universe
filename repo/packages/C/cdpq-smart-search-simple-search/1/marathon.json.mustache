{
   "id": "{{service.name}}",
   "cpus": {{service.cpus}},
   "mem": {{service.mem}},
   "disk":{{service.disk}},
   "instances": {{service.instances}},
   {{#service.cmd}}
   "cmd": "{{service.cmd}}",
   {{/service.cmd}}
   "container":{
      "type": "DOCKER",
      "docker": {
         "forcePullImage": true,
         "image": "{{resource.assets.container.docker.image}}",
         "parameters": [],
         "privileged": false
      },
      "volumes": [],
      "portMappings": [
      {
        "containerPort": {{networking.container_port}},
        "hostPort": {{networking.host_port}},
        "labels": {},
        "protocol": "tcp",
        "servicePort": {{networking.service_port}}
      }
    ]
   },
   "networks":[
      {
         "mode":"container/bridge"
      }
   ],
   "healthChecks": 
   [
    {
      "gracePeriodSeconds": {{health_checks.MESOS_HTTP.advanced_health_checking_settings.grace_periods}},
      "intervalSeconds": {{health_checks.MESOS_HTTP.advanced_health_checking_settings.intervals}},
      "timeoutSeconds": {{health_checks.MESOS_HTTP.advanced_health_checking_settings.timeout}},
      "maxConsecutiveFailures": {{health_checks.MESOS_HTTP.advanced_health_checking_settings.max_failures}},
      "delaySeconds": {{health_checks.MESOS_HTTP.advanced_health_checking_settings.delay}},
      "portIndex": 0,
      "path": "{{health_checks.MESOS_HTTP.path}}",
      "protocol": "MESOS_HTTP"
    }
   ],
   "env": {
      "SMART_SEARCH_URL": "{{environment.SMART_SEARCH_URL}}"
   },
   "labels" : {
     "DCOS_PACKAGE_OPTIONS": "{{environment.labels.DCOS_PACKAGE_OPTIONS}}",
     "DCOS_SERVICE_SCHEME": "{{environment.labels.DCOS_SERVICE_SCHEME}}",
     "DCOS_PACKAGE_SOURCE": "{{environment.labels.DCOS_PACKAGE_SOURCE}}",
     "DCOS_PACKAGE_METADATA": "{{environment.labels.DCOS_PACKAGE_METADATA}}",
     "DCOS_SERVICE_NAME": "{{environment.labels.DCOS_SERVICE_NAME}}",
     "DCOS_SERVICE_PORT_INDEX": "{{environment.labels.DCOS_SERVICE_PORT_INDEX}}",
     "DCOS_PACKAGE_DEFINITION": "{{environment.labels.DCOS_PACKAGE_DEFINITION}}",
     "DCOS_PACKAGE_VERSION": "{{package.version}}",
     "DCOS_PACKAGE_NAME": "{{service.name}}"
   },
  "fetch": 
   [
    {
      "uri": "{{fetch.artifact_URI.uri}}",
      "extract": {{fetch.artifact_URI.extract}},
      "executable": {{fetch.artifact_URI.executable}},
      "cache": {{fetch.artifact_URI.cache}}
    }
   ]
}