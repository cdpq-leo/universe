{
  "id": "/{{service.name}}",
  "instances": {{service.instances}},
  "cpus": {{service.cpus}},
  "gpus": 0,
  "mem": {{service.mem}},
  "disk": {{service.disk}},
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": "/entrypoint.sh worker --no-format",
  "container": {
    "portMappings": [
      {
        "containerPort": 30000,
        "hostPort": 30000,
        "protocol": "tcp",
        "servicePort": 10104
      },
      {
        "containerPort": {{alluxio.worker-data-port}},
        "hostPort": {{alluxio.worker-data-port}},
        "protocol": "tcp",
        "servicePort": {{alluxio.worker-data-port}}
      },
      {
        "containerPort": {{alluxio.worker-port}},
        "hostPort": {{alluxio.worker-port}},
        "protocol": "tcp",
        "servicePort": {{alluxio.worker-port}}
      },
      {
        "containerPort": 39999,
        "hostPort": 10106,
        "protocol": "tcp",
        "servicePort": 10106
      }
    ],
    "type": "DOCKER",
    "volumes": [
      {{#alluxio.tier1}}
      {
        "containerPath": "{{alluxio.tier1-dirs-path}}",
        "hostPath": "{{alluxio.tier1-host-path}}",
        "mode": "RW"
      },
      {{/alluxio.tier1}}
      {
        "containerPath": "/opt/ramdisk",
        "hostPath": "/mnt/ramdisk",
        "mode": "RW"
      }
      {{#alluxio.tier2}},
      {
        "containerPath": "{{alluxio.tier2-dirs-path}}",
        "hostPath": "{{alluxio.tier2-host-path}}",
        "mode": "RW"
      }
      {{/alluxio.tier2}}
    ],
    "docker": {
      "image": "{{service.docker-image}}",
      "forcePullImage": true,
      "privileged": false,
      "parameters": []
    }
  },
  "env": {
    "ALLUXIO_WORKER_PORT": "{{alluxio.worker-port}}",
    "ALLUXIO_WORKER_DATA_PORT": "{{alluxio.worker-data-port}}",
    "ALLUXIO_WORKER_TIEREDSTORE_LEVELS": "{{alluxio.tier-levels}}",
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL0_ALIAS": "MEM",
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL0_DIRS_PATH": "{{alluxio.tier0-dirs-path}}",
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL0_DIRS_QUOTA": "{{alluxio.tier0-quota}}GB",
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL0_WATERMARK_HIGH_RATIO": "{{alluxio.tier0-watermark-high-ratio}}",
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL0_WATERMARK_LOW_RATIO": "{{alluxio.tier0-watermark-low-ratio}}",
    {{#alluxio.tier1}}
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL1_ALIAS": "{{alluxio.tier1-alias}}",
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL1_DIRS_PATH": "{{alluxio.tier1-dirs-path}}",
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL1_DIRS_QUOTA": "{{alluxio.tier1-quota}}GB",
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL1_WATERMARK_HIGH_RATIO": "{{alluxio.tier1-watermark-high-ratio}}",
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL1_WATERMARK_LOW_RATIO": "{{alluxio.tier1-watermark-low-ratio}}",
    {{/alluxio.tier1}}
    {{#alluxio.tier2}}
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL2_ALIAS": "{{alluxio.tier2-alias}}",
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL2_DIRS_PATH": "{{alluxio.tier2-dirs-path}}",
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL2_DIRS_QUOTA": "{{alluxio.tier2-quota}}GB",
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL2_WATERMARK_HIGH_RATIO": "{{alluxio.tier2-watermark-high-ratio}}",
    "ALLUXIO_WORKER_TIEREDSTORE_LEVEL2_WATERMARK_LOW_RATIO": "{{alluxio.tier2-watermark-low-ratio}}",
    {{/alluxio.tier2}}
    "ALLUXIO_UNDERFS_ADDRESS": "{{alluxio.underfs-address}}",
    "ALLUXIO_MASTER_HOSTNAME": "{{alluxio.master-hostname}}",
    "ALLUXIO_UNDERFS_HDFS_CONFIGURATION": "{{alluxio.underfs-hdfs-configuration}}"
  },
  "labels": {
    "DCOS_SERVICE_PORT_INDEX": "0",
    "HAPROXY_GROUP": "internal",
    "DCOS_SERVICE_NAME": "{{service.name}}",
    "DCOS_SERVICE_SCHEME": "http"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "timeoutSeconds": 20,
      "delaySeconds": 15,
      "protocol": "TCP",
      "ipProtocol": "IPv4"
    }
  ],
  "maxLaunchDelaySeconds": 3600,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  }
}
