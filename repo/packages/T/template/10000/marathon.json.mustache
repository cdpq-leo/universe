{
   "id": "{{service.name}}",
   "cpus": {{service.cpus}},
   "mem": {{service.mem}},
   "disk":{{service.disk}},
   "cmd": "{{service.cmd}}",

   "env":{
      "ENV_VAR":"{{environment.ENV_VAR}}"
   },
   "labels":{
      "DCOS_SERVICE_SCHEME": "http",
      "DCOS_SERVICE_NAME": "{{service.name}}",
      "DCOS_SERVICE_PORT_INDEX": "0"
   },
   "container":{
      "portMappings":[
         {
            "containerPort":{{networking.container_port}},
            "hostPort":0,
            "protocol":"tcp"
         }
      ],
      "type":"DOCKER",
      "docker":{
         "image":"{{resource.assets.container.docker.image}}",
         "forcePullImage":false,
         "privileged": false
      }
   },
   "networks":[
      {
         "mode":"{{networking.network_type}}"
      }
   ],
}