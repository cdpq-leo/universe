{
  "id": "{{service.name}}",
  "cpus": {{grakn.cpus}},
  "mem": {{grakn.mem}},
  "disk": {{grakn.disk}},
  "labels": {
    "DCOS_SERVICE_NAME": "{{service.name}}",
    "DCOS_SERVICE_SCHEME": "http",
    "DCOS_SERVICE_PORT_INDEX": "0"
  },
  "env": {
    "STORAGE_IS_LOCAL": "{{storage.is_local}}",
    "STORAGE_BACKEND": "{{storage.backend}}",
    "STORAGE_HOSTNAMES": "{{storage.hostnames}}",
    "INDEX_SEARCH_BACKEND": "{{index.search_backend}}",
    "INDEX_SEARCH_HOSTNAME": "{{index.search_hostname}}",
    "REDIS_IS_LOCAL": "{{redis.is_local}}",
    "REDIS_HOSTNAME": "{{redis.hostname}}"
    "SPARK_MASTER": "{{spark.master}}"
  },
  "constraints": {{{service.constraints}}},
  "container": {
    "portMappings": [
      {
        "containerPort": 4567,
        "hostPort": 0,
        "labels": {
          "VIP_0": "{{service.name}}:4567"
        },
        "protocol": "tcp"
      }
    ],
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "{{resource.assets.container.docker.grakn}}",
      "forcePullImage": true,
      "privileged": false,
      "parameters": []
    }
  },
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/",
      "gracePeriodSeconds": 120,
      "intervalSeconds": 30,
      "maxConsecutiveFailures": 3,
      "timeoutSeconds": 5
  }]
}
